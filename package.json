{
  "name": "@shakacode-tools/react-on-rails-rsc",
  "version": "19.0.0",
  "description": "React Server Components support for react_on_rails Ruby gem",
  "exports": {
    "./client": "./dist/client.js",
    "./server.node": "./dist/server.node.js",
    "./WebpackPlugin": "./dist/WebpackPlugin.js",
    "./WebpackLoader": "./dist/WebpackLoader.js"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist",
    "types",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build-if-needed": "[ -f dist/client.js ] || (npm run build >/dev/null 2>&1 || true) && [ -f dist/client.js ] || { echo 'Build failed'; }",
    "prepack": "npm run build-if-needed",
    "prepare": "npm run build-if-needed"
  },
  "dependencies": {
    "react-server-dom-webpack": "^19.0.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^14.1.2",
    "typescript": "^5.4.3",
    "webpack": "^5.98.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shakacode-tools/react-on-rails-rsc.git"
  },
  "keywords": [
    "server components",
    "rsc",
    "ruby on rails",
    "react-on-rails"
  ],
  "author": "ShakaCode",
  "license": "MIT"
}
