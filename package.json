{
  "name": "@shakacode-tools/react-on-rails-rsc",
  "description": "React Server Components bindings for DOM using Webpack. This is intended to be integrated into meta-frameworks. It is not intended to be imported directly.",
  "version": "19.0.0-rc.2",
  "keywords": [
    "react"
  ],
  "homepage": "https://react.dev/",
  "bugs": "https://github.com/facebook/react/issues",
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "plugin.js",
    "client.d.ts",
    "client.js",
    "client.browser.js",
    "client.edge.js",
    "client.node.d.ts",
    "client.node.js",
    "client.node.unbundled.js",
    "server.js",
    "server.browser.js",
    "server.edge.js",
    "server.node.d.ts",
    "server.node.js",
    "server.node.unbundled.js",
    "static.js",
    "static.browser.js",
    "static.edge.js",
    "static.node.js",
    "static.node.unbundled.js",
    "node-register.js",
    "WebpackLoader.d.ts",
    "WebpackLoader.js",
    "WebpackPlugin.d.ts",
    "WebpackPlugin.js",
    "cjs/",
    "esm/"
  ],
  "exports": {
    "./client": {
      "types": "./client.d.ts",
      "default": "./client.js"
    },
    "./client.node": {
      "types": "./client.node.d.ts",
      "default": "./client.node.js"
    },
    "./server.node": {
      "types": "./server.node.d.ts",
      "default": "./server.node.js"
    },
    "./WebpackPlugin": {
      "types": "./WebpackPlugin.d.ts",
      "default": "./WebpackPlugin.js"
    },
    "./WebpackLoader": {
      "types": "./WebpackLoader.d.ts",
      "default": "./WebpackLoader.js"
    },
    "./package.json": "./package.json",
    "./server": {
      "react-server": {
        "workerd": "./server.edge.js",
        "deno": "./server.browser.js",
        "node": {
          "webpack": "./server.node.js",
          "default": "./server.node.unbundled.js"
        },
        "edge-light": "./server.edge.js",
        "browser": "./server.browser.js"
      },
      "default": "./server.js"
    }
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/facebook/react.git",
    "directory": "packages/react-server-dom-webpack"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "webpack": "^5.59.0"
  },
  "dependencies": {
    "acorn-loose": "^8.3.0",
    "neo-async": "^2.6.1",
    "webpack-sources": "^3.2.0"
  }
}
